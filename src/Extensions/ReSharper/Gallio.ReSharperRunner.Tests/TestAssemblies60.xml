    </ProductEdition>
    <ProductEdition Name="Vbnet" DisplayName="VB.NET" Description="Supports VB.NET, but does not support C#.">
      <AssemblyConfigurationRef Name="Vbnet" />
    </ProductEdition>
  </ProductEditions>
